<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        /* Add CSS here. See the GWT docs on UI Binder for more details */
    </ui:style>
    <g:HTMLPanel>

        <div id="txtTitle">
            <g:SimplePanel ui:field="txtTitle" />
        </div>
        <div class="upload-frame">
            <div id="txtErrorSummary">
                <g:SimplePanel ui:field="txtErrorSummary" />
            </div>
            <div id="frameSelectPJDocuments">
                <g:SimplePanel ui:field="frameSelectPJDocuments" />
            </div>
            <div id="frameTransferPJDocuments">
                <g:SimplePanel ui:field="frameTransferPJDocuments" />
            </div>
        </div>

        <div id="btnCompleteLater">
            <g:SimplePanel ui:field="btnCompleteLater" />
        </div>
        <div id="btnBack">
            <g:SimplePanel ui:field="btnBack" />
        </div>
        <div id="btnTransmit">
            <g:SimplePanel ui:field="btnTransmit" />
        </div>


    </g:HTMLPanel>
</ui:UiBinder> 